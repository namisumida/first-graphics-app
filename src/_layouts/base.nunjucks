<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="styles/main.css">
    {% block stylesheets %}{% endblock %}
</head>
<body>
  <div class="article-body">
    <section class="title">
      <div class="title__text">
        <h1 class="title__title">{% block title__title %}{% endblock %}</h1>
        <div class="title__subhead">{% block title__subhead %}{% endblock %}</div>
        <div class="title__bylinedate">
          <span class="title__date">{% block title__date %}{% endblock %}</span>
          <span class="bullet">&bull;</span>
          <span class="title__byline">{% block title__byline %}{% endblock %}</span>
        </div>
      </div>
      <figure class="inside_story intro__illustration">
        <img src="https://www.retaildive.com/user_media/cache/67/6b/676b02ed2c7cf154eb0968c502a14ac6.jpg"></img>
        <figcaption class="inside_story_caption">
          <div class="source_text">{% block intro__illustration__source %}{% endblock %}</div>
        </figcaption>
      </figure>
    </section>
    {% block content %}{% endblock %}
    {% block scripts %}{% endblock %}
  </div>
  <script src="scripts/main.js"></script>
</body>
</html>

